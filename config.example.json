{
    "EmailAddress": "your_email@gmail.com",
	"ClientId": "your_client_id",
	"ClientSecret": "your_secret",
	"Filters": [
		{
			"By": "From",
			"Op": "Eq",
			"Args": [ "email@test.com" ]
		},
		{
			"By": "From",
			"Op": "Neq",
			"Args": [ "not_this@test.com" ]
		},
		{
			"By": "From",
			"Op": "In",
			"Args": [ "email1@test.com", "email2@test.com", "email3@test.com" ]
		}
	],
	"ConnString": "your_conn_string"
}